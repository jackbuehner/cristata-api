{
  "json.schemas": [
    {
      "fileMatch": ["/*.collection.json"],
      "url": "./src/json-schemas/collection.schema.json"
    }
  ],
  "editor.formatOnSave": true,
  "editor.fontFamily": "'Dank Mono', Consolas, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.tabs.defaultColor": "terminal.ansiYellow",
  "terminal.integrated.tabs.defaultIcon": "terminal-bash"
}
