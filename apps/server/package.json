  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "release": "standard-version",
    "start": "cross-env APP_URL=${APP_URL:=https://cristata.app} AUTH_APP_URL=${AUTH_APP_URL:=https://auth.cristata.app} node dist/index.js",
    "dev:prepare": "tsc",
    "dev": "npm run dev:prepare && tsc --watch & cross-env PORT=${PORT:=3000} APP_URL=${APP_URL:=https://cristata.app} AUTH_APP_URL=${AUTH_APP_URL:=https://auth.cristata.app} NODE_ENV=development nodemon dist/api/index.js",
    "build": "tsc --build && tsc --declaration && cross-env NODE_ENV=production npm run lint",
    "check": "tsc && npm run lint",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist",
    "postinstall": "patch-package --error-on-fail false",
    "heroku-cleanup": "patch-package",
    "watch": "tsc --watch",
    "build:json-schema": "typescript-json-schema ./tsconfig.json-schema.json GenCollectionInput --out ./src/json-schemas/collection.schema.json --defaultProps --noExtraProps --required"
  },
  "standard-version": {
    "scripts": {
      "posttag": "git push --follow-tags origin"
    }
  },
